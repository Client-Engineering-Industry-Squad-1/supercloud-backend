{
  "name": "FS-Cloud-Architectures",
  "version": "0.0.1",
  "description": "Backend for Frontend for managing relationships between architectures, boms, services and automation and fs controls",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js",
    "docker:build": "docker build -t architecture-mapper-bff .",
    "docker:run": "docker run -p 3001:3001 -d architecture-mapper-bff",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "yarn run clean && yarn run build",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Github pipeline <pipeline@github.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@cloudnativetoolkit/iascable": "^0.4.0",
    "@loopback/boot": "^3.2.0",
    "@loopback/core": "^2.14.0",
    "@loopback/repository": "^3.4.0",
    "@loopback/rest": "^9.1.2",
    "@loopback/rest-explorer": "^3.0.6",
    "@loopback/service-proxy": "^3.0.6",
    "@types/figlet": "^1.2.1",
    "@types/nconf": "^0.10.0",
    "asset": "^0.4.13",
    "cross-fetch": "^3.0.6",
    "csvtojson": "^2.0.10",
    "figlet": "^1.5.0",
    "loopback-connector-mongodb": "^5.5.0",
    "nconf": "^0.11.2",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.9",
    "@loopback/eslint-config": "^10.0.5",
    "@loopback/testlab": "^3.2.11",
    "@types/lodash": "^4.14.168",
    "@types/node": "^10.17.51",
    "eslint": "^7.18.0",
    "source-map-support": "^0.5.19",
    "typescript": "~4.1.3"
  }
}
